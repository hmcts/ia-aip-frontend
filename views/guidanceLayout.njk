      <a href="{{ paths.common.guidance | safe }}" class="govuk-link govuk-link--no-visited-state guidance ">{{ i18n.pages.onlineSupportPages.links.guidance | safe }}</a>
    </li>
    <li>
      <a href="{{ paths.common.howToHelp | safe }}" class="govuk-link govuk-link--no-visited-state howToHelp">{{ i18n.pages.onlineSupportPages.links.howToHelp | safe }}</a>
