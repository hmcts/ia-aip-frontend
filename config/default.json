{
  "testUrl": "https://localhost:3000",
  "httpProxy": false,
  "proxy": {
    "host": "proxyout.reform.hmcts.net",
    "port": 8080
  },
  "appInsights": {
    "instrumentationKey": ""
  },
  "node": {
    "protocol": "http",
    "hostname": "localhost",
    "port": 3000
  },
  "ssl": {
    "hpkp": {
      "maxAge": "3600",
      "sha256s": "AbCdEf123=",
      "sha256sBackup": "ZyXwVu456="
    }
  },
  "microservice": "iac",
  "session": {
    "useRedis": true,
    "useLogger": false,
    "redis": {
      "url": "redis://127.0.0.1:6379",
      "secret": "defaultsecret",
      "ttlInSeconds": 1200
    },
    "cookie": {
      "secret": "defaultcookiesecret",
      "maxAgeInMs": 1200000,
      "secure": false
    }
  },
  "s2s": {
    "microserviceName": "S2S_MICROSERVICE_NAME",
    "secret": "S2S_SECRET",
    "url": "S2S_URL"
  },
  "idam": {
    "apiUrl": "IDAM_API_URL",
    "webUrl": "IDAM_WEB_URL",
    "secret": "IDAM_SECRET"
  },
  "ccd": {
    "apiUrl": "CCD_API_URL",
    "jurisdictionId": "IA",
    "caseType": "Asylum"
  },
  "documentManagement": {
    "apiUrl": "DOC_MANAGEMENT_URL"
  },
  "addressLookup": {
    "token": "address_lookup_token",
    "url": "https://api.ordnancesurvey.co.uk"
  },
  "saucelabs": {
    "secret": "SAUCELABS_SECRET"
  },
  "functionalTests": "*.feature",
  "showTests": false,
  "maxCharacters": 50000,
  "evidenceUpload": {
    "maxFileSizeInMb": 100,
    "supportedFormats": [
      ".jpg", ".jpeg", ".bmp", ".tif", ".tiff", ".png",
      ".pdf", ".txt", ".doc", ".dot", ".docx", ".dotx",
      ".xls", ".xlt", ".xla", ".xlsx", ".xltx", ".xlsb",
      ".ppt", ".pot", ".pps", ".ppa", ".pptx", ".potx",
      ".ppsx", ".rtf", ".csv"
    ]
  },
  "daysToWait": {
    "afterSubmission": 28,
    "afterReasonsForAppeal": 14,
    "afterCQSubmission": 7
  },
  "features": {
    "askForMoreTime": false,
    "timelineEnabled": false
  },
  "testAccounts": {
    "testCaseOfficerUserName": "TEST_CASEOFFICER_USERNAME",
    "testCaseOfficerPassword": "TEST_CASEOFFICER_PASSWORD",
    "testHomeOfficeGenericUserName": "TEST_HOMEOFFICE_GENERIC_USERNAME",
    "testHomeOfficeGenericPassword": "TEST_HOMEOFFICE_GENERIC_PASSWORD"
  }
}
