{% set pageTitleName = i18n.error.error404.title %}
{% extends "layout.njk" %}
{% set pageHeader = i18n.error.error404.header %}
{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <h1 class="govuk-heading-xl">{{ i18n.error.error404.header }}</h1>
      <p class="govuk-body">
        {{ i18n.error.error404.webAddress }}
      </p>
      <p class="govuk-body">
        {{ i18n.error.error404.pasted }}
      </p>
      <p class="govuk-body">
        {{ i18n.error.error404.link }}
        {{ contactUsErrorLink() }}
      </p>
    </div>
  </div>
{% endblock %}
