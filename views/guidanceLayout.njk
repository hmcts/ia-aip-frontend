{% endmacro %}

{% macro backForward(back,forward) %}
  <div style="display: inline-block">
    <a class="govuk-link govuk-link--no-visited-state"  href={{ back }}>< Previous page</a>
    <a class="govuk-link govuk-link--no-visited-state floater" style="  margin-left:300px; margin-right:0" href={{ forward }}> Next page ></a>
  </div>
{% endmacro %}

