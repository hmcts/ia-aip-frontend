{% macro navigation() %}
  <ul class="govuk-list">
    <li>
      <a href="{{ paths.common.whatIsIt | safe }}" class="govuk-link govuk-link--no-visited-state whatIsIt">{{ i18n.pages.onlineSupportPages.links.whatIsThisService | safe }}</a>
    </li>
    <li>
      <a href="{{ paths.common.gettingStarted | safe }}" class="govuk-link govuk-link--no-visited-state gettingStarted">{{ i18n.pages.onlineSupportPages.links.gettingStarted | safe }}</a>
    </li>
    <li>
      <a href="{{ paths.common.fourStages | safe }}" class="govuk-link govuk-link--no-visited-state fourStages">{{ i18n.pages.onlineSupportPages.links.fourStages | safe }}</a>
    </li>
    <li>
      <a href="{{ paths.common.notifications | safe }}" class="govuk-link govuk-link--no-visited-state notifications">{{ i18n.pages.onlineSupportPages.links.notifications | safe }}</a>
    </li>
    <li>
      <a href="{{ paths.common.documents | safe }}" class="govuk-link govuk-link--no-visited-state documents">{{ i18n.pages.onlineSupportPages.links.documents | safe }}</a>
